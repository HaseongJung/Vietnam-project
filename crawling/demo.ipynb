    "def getCategory(soup):\n",
    "    try:\n",
    "        category = ''\n",
    "        for x in soup.find('span', class_=\"DsyBj DxyfE\"):\n",
    "            if '$' not in x.text:\n",
    "                category += f'{x.text}, '\n",
    "        category = category[:-2]\n",
    "    except:\n",
    "        category = None\n",
    "\n",
    "    return (category)"
   ]
  },
  {
   "cell_type": "code",
